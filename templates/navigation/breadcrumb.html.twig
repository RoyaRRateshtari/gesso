{#
/**
 * @file
 * Theme override for a breadcrumb trail.
 *
 * Available variables:
 * - breadcrumb: Breadcrumb trail items.
 */
#}

{% if breadcrumb %}
  {%
    include '@components/breadcrumb/breadcrumb.twig' with {
      modifier: "",
      heading: 'Breadcrumb'|t,
      breadcrumb: breadcrumb
    } only
  %}
{% endif %}

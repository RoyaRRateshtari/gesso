{#
  /**
  * @file
  * Component for a Page.
  *
  * Variables:
  * - attributes: [array] HTML attributes for the page
  *   - class: [string] any classed added by user or system
  */
#}

<article{{- attributes.addClass(classes)|raw -}}>
  {% block main %}
    {{ page_title|raw }}
    {{ page_content|raw }}
  {% endblock %}
</article>

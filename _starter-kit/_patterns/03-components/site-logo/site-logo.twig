{#
  /**
  * @file
  * Component for Site Logo.
  *
  * Available variables:
  * - url: [string] The site link url.
  * - site_logo: [string] The path to logo image.
  */
#}

<a class="site-logo" href="{{ url }}" title="{{ 'Home'|t }}" rel="home">
  <img class="site-logo__image" src="{{ site_logo }}" alt="{{ 'Home'|t }}" />
</a>
